{
    "name": "meepo",
    "version": "1.0.0",
    "description": "meepo framework",
    "main": "index.js",
    "scripts": {
        "test": "test",
        "runner.start": "ng server --app runner-demo --port 4201",
        "admin.start": "ng server --app admin-demo --port 4202",
        "admin.build": "ng build --app admin-demo --prod",
        "core.start": "ng ftp:runner && ng server --app core-demo",
        "ftp:runner": "gulp model.pages",
        "ftp:inc": "gulp member",
        "desktop": "gulp admin:window:build && gulp admin:mac:build",
        "android": "rm -rf .cordova/ && yarn admin.build && gulp admin:android:build && cd .cordova/ && cordova platform add android && cordova build",
        "window": "electron-packager ./dist/admin/ runner --platform=win32 --arch=all",
        "mac": "electron-packager ./dist/admin/ runner --platform=darwin --arch=all ",
        "mac:install": "electron-installer-dmg ./desktop/runner-darwin-x64/runner.app runner"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/meepobrother/meepo.git"
    },
    "keywords": [
        "meepo",
        "framework",
        "angular",
        "exporess",
        "socket.io"
    ],
    "author": "imeepos",
    "license": "ISC",
